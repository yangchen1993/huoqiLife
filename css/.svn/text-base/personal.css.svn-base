.content{
    font-family: 微软雅黑;
}
.personal_info{
    height: 180px;
    background: url('../img/test3.png') no-repeat;
    position: relative;
}
.personal_info img{
    width: 90px;
    height: 90px;
    border-radius: 50%;
}
.personal_info p{
    color: #fff;
    font-size: 16px;
    margin: 10px 0 0;
}
.personal_info1{
    position: absolute;
    top:50%;
    margin-top: -63px;
    left: 50%;
    margin-left: -47px;
}
.personal_info1 div{
    display: inline-block;
    border: 2px solid #eee;
    border-radius: 50%;
}


.content1{
    /*background-color: #e5e4e4;*/
    /*padding-top: 6px;*/
}
.content1_tel{
    background-color: #fff;
    padding: 13px 0 9px;
}
.content1_tel p{
    color:#8f8f8f;
    margin: 0;
    font-size: 16px;
}
.content1_tel img{
    margin:-5px 10px 0 0;
}
.content1_operation{
    display: table-cell;
    background-color: #fff;
    box-sizing: border-box;
    border-top: 1px solid #e5e4e4;
}
.content1_operation>div{
    text-align: center;
    padding: 5% 0;
    border-right: 1px solid #e5e4e4;
    border-bottom: 1px solid #e5e4e4;
}
.content1_operation p{
    margin: 10px 0 0;
    color: #5b5b5b;
}


.content1_op{
    position: relative;
    height: 45px;
    /*line-height: 50px;*/
    padding:0 10px;
    background-color: #fff;
    border-bottom: 1px solid #e4e1e1;
}
.content1_op i:nth-child(1){
    font-size: 22px;
    position: absolute;
    top:50%;
    margin-top: -11px;
    line-height: 1;
}
.content1_op i:nth-child(3){
    /*font-size: 20px;*/
    line-height: 1;
    color: #c2c2c2;
    margin-top: 17px;
}
.content1_op span{
    position: absolute;
    top: 50%;
    left: 40px;
    line-height: 1;
    margin-top: -8px;
    font-size: 16px;
    color: #222;
}
.cut_line{
    height: 6px;
    background-color: #e4e1e1;
}